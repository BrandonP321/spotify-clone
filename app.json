{
  "expo": {
    "name": "spotify-clone",
    "slug": "spotify-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spotify-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brandonp321.spotifyclone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/stores/google-play/app-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.brandonp321.spotifyclone"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "myapp"
  }
}
